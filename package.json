{
  "name": "202020-website",
  "version": "1.0.0",
  "description": "202020 Timer - Mac menu bar app",
  "main": "sass/mystyles.scss",
  "scripts": {
    "css-build": "node-sass --omit-source-map-url sass/mystyles.scss public/css/mystyles.css",
    "css-minify": "postcss public/css/mystyles.css -o public/css/mystyles.css",
    "css-watch": "npm run css-build -- --watch",
    "start": "npm run css-watch"
  },
  "author": "logan.williams",
  "devDependencies": {
    "bulma": "^0.9.0",
    "cssnano": "^4.1.10",
    "node-sass": "^4.14.1"
  }
}
